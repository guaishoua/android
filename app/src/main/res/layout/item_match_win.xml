<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.roundwidget.QMUIRoundConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_108"
    android:padding="@dimen/dp_10"
    app:qmui_backgroundColor="@color/color_white"
    app:qmui_radius="@dimen/dp_1">

    <TextView
        android:id="@+id/tv_rank_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/rank"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/lin_uid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_30"
        android:orientation="vertical"
        app:layout_constraintLeft_toRightOf="@+id/tv_rank_title"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tv_uid_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/matchreslut_tag3_text1"
            android:textColor="@color/color_match2"
            android:textSize="@dimen/text_size_sp_11" />

        <TextView
            android:id="@+id/tv_uid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:textColor="@color/color_match2"
            android:textSize="@dimen/text_size_sp_11"
            tools:text="111" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin_tradeAmount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:orientation="vertical"
        app:layout_constraintLeft_toRightOf="@+id/lin_uid"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_min="@dimen/dp_100">

        <TextView
            android:id="@+id/tv_tradeAmount_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/trade_amount"
            android:textColor="@color/color_match2"
            android:textSize="@dimen/text_size_sp_11" />

        <TextView
            android:id="@+id/tv_tradeAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:textColor="@color/color_match2"
            android:textSize="@dimen/text_size_sp_11"
            tools:text="111" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_rank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_6"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_rank_title"
        tools:text="111" />

    <TextView
        android:id="@+id/tv_all_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_15"
        android:text="@string/all_amount"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tv_reward_title"
        app:layout_constraintTop_toBottomOf="@id/tv_rank" />

    <TextView
        android:id="@+id/tv_reward_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/matchreslut_tag3_text2"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintBottom_toBottomOf="@+id/tv_all_title"
        app:layout_constraintLeft_toRightOf="@+id/tv_all_title"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/tv_all"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_6"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintLeft_toLeftOf="@id/tv_all_title"
        app:layout_constraintTop_toBottomOf="@id/tv_all_title"
        tools:text="111" />

    <TextView
        android:id="@+id/tv_reward"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_6"
        android:textColor="@color/color_match2"
        android:textSize="@dimen/text_size_sp_11"
        app:layout_constraintLeft_toLeftOf="@id/tv_reward_title"
        app:layout_constraintTop_toBottomOf="@id/tv_reward_title"
        tools:text="111" />
</com.qmuiteam.qmui.widget.roundwidget.QMUIRoundConstraintLayout>