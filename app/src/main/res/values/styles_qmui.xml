<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--去掉QMUIRoundButton里的边的颜色 不改的话会用框架默认地蓝色-->
    <style name="QMUIRoundButtonCustom" parent="QMUI.RoundButton">
        <item name="qmui_borderWidth">0dp</item>
        <item name="qmui_borderColor">@color/color_transparent</item>
    </style>

    <!--QMUITopBar的样式设置-->
    <style name="QMUITopBarCustom" parent="QMUI.TopBar">
        <item name="qmui_topbar_separator_color">@color/content_bg_color_grey</item>
        <item name="qmui_topbar_separator_height">@dimen/dp_2</item>
        <item name="qmui_topbar_bg_color">@color/content_bg_color</item>
        <item name="qmui_topbar_title_color">@color/text_white</item>
        <item name="qmui_topbar_text_btn_color_state_list">@color/text_white</item>
    </style>

    <style name="QMUICommonListItemViewCustom" parent="QMUI.CommonListItemView">
        <item name="qmui_commonList_titleColor">@color/text_color</item>
        <item name="qmui_commonList_detailColor">@color/text_white_50</item>
    </style>
</resources>